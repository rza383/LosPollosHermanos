<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Widget.LosPollos.Divider" parent="">
        <item name="android:background">?attr/dividerHorizontal</item>
    </style>

    <style name="Widget.LosPollos.TextView.Label" parent="Widget.MaterialComponents.TextView">
        <item name="android:textAllCaps">true</item>
        <item name="android:textAppearance">?attr/textAppearanceBody1</item>
    </style>

    <style name="Widget.LosPollos.TextView.OrderSummary" parent="Widget.MaterialComponents.TextView">
        <item name="android:textAppearance">?attr/textAppearanceBody1</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="Widget.LosPollos.TextView" parent="Widget.MaterialComponents.TextView">
        <item name="android:textAppearance">?attr/textAppearanceSubtitle1</item>
    </style>

    <style name="Widget.Unscramble.TextInputLayout.OutlinedBox" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeErrorColor">@color/yellow_600</item>
        <item name="errorIconTint">@color/yellow_600</item>
        <item name="errorTextColor">@color/yellow_600</item>
        <item name="errorIconDrawable">@drawable/ic_error</item>
        <item name="helperTextTextAppearance">@style/TextAppearance.MaterialComponents.Subtitle1</item>
    </style>
</resources>